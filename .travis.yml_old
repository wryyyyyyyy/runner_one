language: bash
os: linux

dist: xenial

rvm:
  2.5

env:
  global:


install:
  - rvm $(travis_internal_ruby) --fuzzy do ruby -S gem install dpl

before_install:
  - gem update --system
  - gem --version

build:
  - rvm $(travis_internal_ruby) --fuzzy do ruby -S gem update
  - sudo /bin/sh -c build.sh

before_deploy:
  - echo pooque

deploy:
  provider: pages
  skip_cleanup: true
  github_token: $GITHUB_TOKEN
  keep_history: true

  -echo srenque

  on:
    branch: gh-pages

branches:
    only:
    - gh-pages
